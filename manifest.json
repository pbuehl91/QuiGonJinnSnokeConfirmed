{

  "name": "Qui Gon Jinn Snoke Confirmed",
  "version": "0.1",
  "description": "Change all pictures on your screen to Qui Gon Jinn",
  "icons": { "16": "icons/icon16.png",
             "48": "icons/icon48.png",
            "128": "icons/icon128.png" },
  "default_locale": "en",

  
  "app": {
    "launch": {
      "local_path": "src/main.html"
    }
  },
  

  "permissions": [
    "http://*.google.com/",
    "bookmarks", 
    "chrome://favicon/",
    "clipboardRead", 
    "clipboardWrite", 
    "contextMenus", 
    "cookies", 
    "geolocation",
    "history", 
    "idle", 
    "management",
    "notifications", 
    "tabs", 
    "tts", 
    "ttsEngine", 
    "unlimitedStorage"
  ],

 //  "browser_action": {
 //    "default_icon": {
 //        "19": "icons/icon16.png",
 //        "38": "icons/icon48.png"
 //    },
 //    "default_title": "That's the tool tip"
 // },




  "content_security_policy": "script-src https://api.twitter.com; object-src 'self'",

  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "css": ["src/contentStyle.css"],
      "js": ["lib/jquery.min.js", "src/contentScript.js"]
    }
  ],
  
  "manifest_version": 2


}
